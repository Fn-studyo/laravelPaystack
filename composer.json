{
    "name": "pipedev/laravel-paystack",
    "description": "A simple laravel package for laravel 7 & 8 that gives php developers to paystack api",
    "license": "MIT",
    "keywords": [
        "php",
        "github",
        "Muritala David",
        "paystack",
        "paystack.co",
        "laravel",
        "rave-flutterwave",
        "laravel 7",
        "laravel 8"
    ],
    "authors": [
        {
            "name": "Muritala David",
            "email": "davmuri1414@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "PipeDev\\LaravelPaystack\\": "packages/PipeDev/LaravelPaystack/src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PipeDev\\LaravelPaystack\\LaravelPaystackServiceProvider"
            ],
            "aliases": {
                "LaravelPaystack": "PipeDev\\LaravelPaystack\\Facades\\LaravelPaystackFacade"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^8.40"
    }
}
